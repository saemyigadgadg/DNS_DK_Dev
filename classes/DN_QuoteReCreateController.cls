public with sharing class DN_QuoteReCreateController {
    public static void quoteReCreate(List<String> lineItemList, String val){
        Set<Id> quoteLineId = new Set<Id>();
        for(Integer i = 0; i < lineItemList.size(); i++){ 
            quoteLineId.add(lineItemList[i]);
        }
        List<QuoteLineItem> erpQuotation = [SELECT Id, ERPQuotationNo__c, QuoteId,Quote_RecordType__c FROM QuoteLineItem WHERE Id IN: quoteLineId];
        if(erpQuotation[0].ERPQuotationNo__c != null){
            List<QuoteLineItem> erpQuotationCount = [SELECT Id, ERPQuotationNo__c, QuoteId,Quote_RecordType__c FROM QuoteLineItem WHERE ERPQuotationNo__c =: erpQuotation[0].ERPQuotationNo__c];

            Set<Id> cloneQuoteCheck = new Set<Id>();
            for(QuoteLineItem qt : erpQuotation){
                cloneQuoteCheck.add(qt.QuoteId); //같은 ERP Quoatation No인데 Quote Id가 두개 이상이면 복제된 건
            }
            List<QuoteLineItem> cloneCheck = [SELECT Id FROM QuoteLineItem WHERE ERPQuotationNo__c =: erpQuotation[0].ERPQuotationNo__c];

            //DNSA경우
            if(erpQuotation[0].Quote_RecordType__c.contains('DNSA')){//clone되어서 동일한 erp Quotation No가 있다면
                if(cloneQuoteCheck.size() > 1 ){
                    //D flag가 있는 Factory Option 조회 조회
                    List<DNSA_Factory_Option_Junction__c> dFacOption = [SELECT Id FROM DNSA_Factory_Option_Junction__c WHERE Quote_Line_Item__c IN: quoteLineId AND UPDATEFLAG__c ='D'];
                    if(!dFacOption.isEmpty())
                    {
                        delete dFacOption;
                    }

                    //D flag가 있는 추가자재 조회
                    List<Accessory__c> dAccessory = [SELECT Id FROM Accessory__c WHERE QuoteLineItem__c IN:quoteLineId AND Delete__c = 'D'];
                    if(!dAccessory.isEmpty())
                    {
                        delete dAccessory;
                    }

                    //D 이외의 추가자재 조회
                    List<Accessory__c> notdAccessory = [SELECT Id, Delete__c, POSNR__c FROM Accessory__c WHERE QuoteLineItem__c IN:quoteLineId AND Delete__c != 'D'];
                    List<Accessory__c> resetAcc = new List<Accessory__c>();
                    if(!notdAccessory.isEmpty()){
                        for(Accessory__c acc : notdAccessory){
                            Accessory__c uprecord = new Accessory__c();
                            uprecord.Id = acc.Id;
                            uprecord.Delete__c = '';
                            uprecord.POSNR__c = null;
                            resetAcc.add(uprecord);
                        }
                    }
                    if(!resetAcc.isEmpty())
                    {
                        update resetAcc;
                    }

                    //D flag가 있는 SQ 조회
                    List<SQ_QuoteLineitem_Junction__c> dSQ = [SELECT Id FROM SQ_QuoteLineitem_Junction__c WHERE QuoteLineItem__c IN:quoteLineId AND UPDATEFLAG__c = 'D'];
                    if(!dSQ.isEmpty())
                    {
                        delete dSQ;
                    }

                    //D 이외의 SQ조회
                    List<SQ_QuoteLineitem_Junction__c> notdSQ = [SELECT Id , UPDATEFLAG__c, POSNR__c FROM SQ_QuoteLineitem_Junction__c WHERE QuoteLineItem__c IN:quoteLineId AND UPDATEFLAG__c != 'D'];
                    List<SQ_QuoteLineitem_Junction__c> resetSQ = new List<SQ_QuoteLineitem_Junction__c>();
                    if(!notdSQ.isEmpty()){
                        for(SQ_QuoteLineitem_Junction__c sq : notdSQ){
                            SQ_QuoteLineitem_Junction__c uprecord = new SQ_QuoteLineitem_Junction__c();
                            uprecord.Id = sq.Id;
                            uprecord.UPDATEFLAG__c = '';
                            uprecord.POSNR__c = null;
                            resetSQ.add(uprecord);
                        }
                    }
                    if(!resetSQ.isEmpty())
                    {
                        update resetSQ;
                    }

                    List<T_COND_ELMT__c> delELMTList = [SELECT Id FROM T_COND_ELMT__c WHERE Quote_Line_Item__c IN:quoteLineId];
                    if(!delELMTList.isEmpty()){
                        delete delELMTList;
                    }


                    List<QuoteLineItem> upqtLineLIst = new List<QuoteLineITem>();
                    for(String id : quoteLineId){
                        QuoteLineItem upQtLine = new QuoteLineItem();
                        upQtLine.Id = id;
                        upQtLine.Max_POSNR__c = 20;
                        upQtLine.ReERPQuotation__c = true;
                        upQtLine.QuotePriceIF__c = false;
                        upqtLineLIst.add(upQtLine);
                    }

                    if(!upqtLineLIst.isEmpty()){
                        update upqtLineLIst;
                    }
                }
                
            }else{
                //선택 되어 넘어온 QuoteLineItem갯수보다 ERP Quotation으로 조회했을때의 레코드 수가 더 많다면
                //복제 or 다중 생성 된 것중 일부만 선택한 건이기 때문에 다시 ERP 견적 생성을 해야함
                //CV저장하면 무조건 초기화
                if(erpQuotationCount.size() > erpQuotation.size() || val == 'CV'){
                    //D flag가 있는 추가자재 조회
                    List<Accessory__c> dAccessory = [SELECT Id FROM Accessory__c WHERE QuoteLineItem__c IN:quoteLineId AND Delete__c = 'D'];
                    if(!dAccessory.isEmpty())
                    {
                        delete dAccessory;
                    }

                    //D 이외의 추가자재 조회
                    List<Accessory__c> notdAccessory = [SELECT Id, Delete__c, POSNR__c FROM Accessory__c WHERE QuoteLineItem__c IN:quoteLineId AND Delete__c != 'D'];
                    List<Accessory__c> resetAcc = new List<Accessory__c>();
                    if(!notdAccessory.isEmpty()){
                        for(Accessory__c acc : notdAccessory){
                            Accessory__c uprecord = new Accessory__c();
                            uprecord.Id = acc.Id;
                            uprecord.Delete__c = '';
                            uprecord.POSNR__c = null;
                            resetAcc.add(uprecord);
                        }
                    }
                    if(!resetAcc.isEmpty())
                    {
                        update resetAcc;
                    }

                    //D flag가 있는 SQ 조회
                    List<SQ_QuoteLineitem_Junction__c> dSQ = [SELECT Id FROM SQ_QuoteLineitem_Junction__c WHERE QuoteLineItem__c IN:quoteLineId AND UPDATEFLAG__c = 'D'];
                    if(!dSQ.isEmpty())
                    {
                        delete dSQ;
                    }

                    //D 이외의 SQ조회
                    List<SQ_QuoteLineitem_Junction__c> notdSQ = [SELECT Id , UPDATEFLAG__c, POSNR__c FROM SQ_QuoteLineitem_Junction__c WHERE QuoteLineItem__c IN:quoteLineId AND UPDATEFLAG__c != 'D'];
                    List<SQ_QuoteLineitem_Junction__c> resetSQ = new List<SQ_QuoteLineitem_Junction__c>();
                    if(!notdSQ.isEmpty()){
                        for(SQ_QuoteLineitem_Junction__c sq : notdSQ){
                            SQ_QuoteLineitem_Junction__c uprecord = new SQ_QuoteLineitem_Junction__c();
                            uprecord.Id = sq.Id;
                            uprecord.UPDATEFLAG__c = '';
                            uprecord.POSNR__c = null;
                            resetSQ.add(uprecord);
                        }
                    }
                    if(!resetSQ.isEmpty())
                    {
                        update resetSQ;
                    }

                    List<T_COND_ELMT__c> delELMTList = [SELECT Id FROM T_COND_ELMT__c WHERE Quote_Line_Item__c IN:quoteLineId];
                    if(!delELMTList.isEmpty()){
                        delete delELMTList;
                    }


                    List<QuoteLineItem> upqtLineLIst = new List<QuoteLineITem>();
                    for(String id : quoteLineId){
                        QuoteLineItem upQtLine = new QuoteLineItem();
                        upQtLine.Id = id;
                        upQtLine.Max_POSNR__c = 20;
                        upQtLine.ReERPQuotation__c = true;
                        upQtLine.QuotePriceIF__c = false;
                        upqtLineLIst.add(upQtLine);
                    }
                    if(!upqtLineLIst.isEmpty()){
                        update upqtLineLIst;
                    }
                }
            }
                
            // }
        }
    }
