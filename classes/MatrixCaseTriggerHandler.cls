    public override void onAfterUpdate(List<sObject> olds, List<sObject> news, Map<Id, sObject> oldMap, Map<Id, sObject> newMap) {
        if(isExecuting) {
            System.debug('TaskTriggerHandler.onAfterUpdate');
            List<Case>   caseList     = (List<Case>) news;
            for(Case c : caseList){
                Case oldObj = (Case)oldMap.get(c.Id);
                System.debug('after OLD Trigger WhatID : '+oldObj.OwnerId);
                System.debug('after NEW Trigger WhatID : '+c.OwnerId);
            }
        }
    }
