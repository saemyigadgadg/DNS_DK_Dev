public with sharing class DN_OpportunityLineEditCallout {
    public class InquiryQueueable implements Queueable, Database.AllowsCallouts {
        private String inquiryErp;
        private String optyId;
        private String mainIndustry;
        private String subIndustry;
        private List<QuoteLineItem> prdInfo;
        private List<QuoteLineItem> quoteprdInfo;
        private Integer currentIndex;

        public InquiryQueueable(String inquiryErp, String optyId, String mainIndustry, String subIndustry, List<QuoteLineItem> prdInfo,List<QuoteLineItem> quoteprdInfo, Integer currentIndex) {
            this.inquiryErp = inquiryErp;
            this.optyId = optyId;
            this.mainIndustry = mainIndustry;
            this.subIndustry = subIndustry;
            this.prdInfo = prdInfo;
            this.quoteprdInfo = quoteprdInfo;
            this.currentIndex = currentIndex;
        }

        public void execute(QueueableContext context) {
            

            if(optyId != null){
                if (currentIndex >= prdInfo.size()) {
                    return; // 모든 Callout이 완료된 경우 종료
                }
            }else{
                if (currentIndex >= quoteprdInfo.size()) {
                    return; // 모든 Callout이 완료된 경우 종료
                }
            }
            
            QuoteLineItem currentProduct;
            QuoteLineItem currentquoteProduct;
            // Callout 요청 생성
            if(optyId != null){
                currentProduct = prdInfo[currentIndex];
            }else{
                currentquoteProduct = quoteprdInfo[currentIndex];
            }
            IF_ERP_Inquiry_Classes.IF_OPTY_007_Req req = new IF_ERP_Inquiry_Classes.IF_OPTY_007_Req();
            if(optyId != null){
                req.I_VBELN = currentProduct.ERPInquiryNo__c;
            }else{
                req.I_VBELN = currentquoteProduct.ERPInquiryNo__c;
            }

            if(optyId != null){
                req.I_VDATU = String.valueOf(currentProduct.RequestedDelivDate__c).replace('-','');
            }else{
                req.I_VDATU = String.valueOf(currentquoteProduct.RequestedDelivDate__c).replace('-','');
            }


            if(optyId != null){
                req.I_ZINDUS = mainIndustry;
                req.I_ZINDUS2 = subIndustry;
                // req.I_ZINDUS = null;
                // req.I_ZINDUS2 = null;
            }else{
                req.I_ZINDUS = null;
                req.I_ZINDUS2 = null;
            }

            // Callout 실행
            IF_ERP_Inquiry instance = new IF_ERP_Inquiry();
            if(!System.Test.isRunningTest()){
                IF_ERP_Inquiry_Classes.IF_OPTY_007_Res response = instance.IF_OPTY_007(req);
                System.debug('Response Message: ' + response);
            }

            // 다음 Callout 예약
            
            if(optyId != null){
                if(System.Test.isRunningTest()){
                    return;
                }
                if (currentIndex + 1 < prdInfo.size()) {
                    System.enqueueJob(new InquiryQueueable(inquiryErp, optyId, mainIndustry,subIndustry, prdInfo, null, currentIndex + 1));
                }
            }else{
                if (currentIndex + 1 < quoteprdInfo.size()) {
                    System.enqueueJob(new InquiryQueueable(inquiryErp, optyId, mainIndustry,subIndustry, null, quoteprdInfo, currentIndex + 1));
                }
            }
            
        }
    }

    @future(callout=true)
    public static void editInquiry(String inquiryErp, String optyId, String mainIndustry, String subIndustry ,Set<String> inquiryNoSet) {
        // 제품 정보 조회
        if(optyId != null){
            List<QuoteLineItem> getIFprdinfo = [SELECT Id, ERPInquiryNo__c, ExpectedDelivDate__c, RequestedDelivDate__c FROM QuoteLineItem WHERE Quote.OpportunityId =: optyId and Quote.Recently_Version__c = true];
            // 첫 번째 작업 예약
            System.enqueueJob(new InquiryQueueable(inquiryErp, optyId, mainIndustry, subIndustry, getIFprdinfo, null, 0));
        }else{
            List<QuoteLineItem> getIFprdinfo = new List<QuoteLineItem>();
            if(!inquiryNoSet.isEmpty()){
                getIFprdinfo = [SELECT Id, ERPInquiryNo__c, ExpectedDelivDate__c, RequestedDelivDate__c FROM QuoteLineItem WHERE ERPInquiryNo__c IN: inquiryNoSet and Quote.Recently_Version__c = true];
            }else{
                getIFprdinfo = [SELECT Id, ERPInquiryNo__c, ExpectedDelivDate__c, RequestedDelivDate__c FROM QuoteLineItem WHERE ERPInquiryNo__c =: inquiryErp and Quote.Recently_Version__c = true];
            }
            // 첫 번째 작업 예약
            System.enqueueJob(new InquiryQueueable(inquiryErp, optyId, mainIndustry, subIndustry, null, getIFprdinfo, 0));
        }
    }
