public with sharing class DN_Quote_LineItem_History_Controller {
    @AuraEnabled(cacheable=true)
    public static List<Map<String, Object>> getQuoteLineItemsHistory(Id recordId) {

        Integer num = 1;
        List<QuoteLineItem> quoteId = [SELECT 
                                        Id
                                       FROM QuoteLineItem
                                       WHERE QuoteId = :recordId];
        System.debug('recordId : ' + recordId);
        System.debug('quoteId : ' + quoteId);

        List<QuoteLineItemHistory> quoteHistory = new List<QuoteLineItemHistory>();
        List<Map<String, Object>> historyList = new List<Map<String, Object>>();

        if(!quoteId.isEmpty()){
            quoteHistory = [SELECT
                                Id,
                                QuoteLineItem.Product2.Name,
                                CreatedBy.Name,
                                CreatedDate,
                                Field,
                                OldValue,
                                NewValue 
                            FROM QuoteLineItemHistory 
                            WHERE QuoteLineItemId IN: quoteId];
            System.debug('quoteHistory : ' + quoteHistory);

            if(!quoteHistory.isEmpty()){
                for (QuoteLineItemHistory record : quoteHistory) {
                    Map<String, Object> recordMap = new Map<String, Object>();
                    recordMap.put('Id', num);
                    recordMap.put('ProductName', record.QuoteLineItem.Product2.Name);
                    recordMap.put('CreatedDate', record.CreatedDate);
                    recordMap.put('Field', record.Field);
                    recordMap.put('CreatedByName', record.CreatedBy.Name);
                    recordMap.put('OldValue', record.OldValue);
                    recordMap.put('NewValue', record.NewValue);
        
                    historyList.add(recordMap);
                    num++;
                }
            }
        }
        return historyList;

        
    }
