public with sharing class DN_RFQCallout {

    @future(callout=true)
    public static void callRFQ(String Id, 
                                Boolean IsInterfaced,
                                String I_INQUIRY_NO, 
                                String I_VERSION,
                                String I_RFQ_ACCEPT_DATE,
                                String I_REVIEW_REQUEST_DATE,
                                String I_BRANCH_NAME,
                                String I_SALES_PERSON,
                                String I_CUSTOMER_COMPANY,
                                String I_DEALER_CODE,
                                String I_PRODUCT_NAME,
                                String I_OUTPUT,
                                String I_WORKING_HOURS,
                                String I_DUTY_DAY,
                                String I_OPERATION_RATE,
                                String I_CYCLE_IME_REQUEST,
                                String I_REMARK,
                                String I_EMAIL,
                                String I_TEL,
                                String I_WRITER,
                                String I_IF_DATE,
                                String I_INQUIRY_SEQ,
                                String I_INQUIRY_DATE,
                                String i_STATUS,
                                String I_ADDRESS,
                                String I_INQUIRY_ID,
                                String I_ORDER_STATUS,
                                String I_RESULT,
                                String I_MODEL_NAME,
                                String I_SE_PERSON,
                                String I_SE_PERSON_NO,
                                String I_RESULT_REMARK
                                ){

        PS__c psInfo = 
        [
            SELECT 
                Owner.Name,
                AEUser__r.Name,
                Opportunity__r.Owner.Account.CustomerCode__c,
                AEUser__r.CustomerCode__c,
                Type__c, 
                MaterialHardness__c, 
                ProcessesBeforeMachining__c, 
                MachineType__c, 
                Quantity__c, 
                ChuckType__c, 
                FixtureClamp__c, 
                RecommendedToolMaker__c, 
                AutomationType__c, 
                LoadCapacity__c
            FROM
                PS__c
            WHERE
                Id =: Id
            LIMIT 1
        ];

        String I_CATEGORY           = psInfo.Type__c;
        String I_MATERIAL_QUALITY   = psInfo.MaterialHardness__c;
        String I_PRE_PROCESS        = psInfo.ProcessesBeforeMachining__c;
        String I_BODY_TYPE          = psInfo.MachineType__c;
        String I_MACHINE_AMOUNT     = String.valueOf(psInfo.Quantity__c);
        String I_MEASURE_TYPE       = psInfo.ChuckType__c;
        String I_CLAMP              = psInfo.FixtureClamp__c;
        String I_RECOMD_COMPANY     = psInfo.RecommendedToolMaker__c;
        String I_AUTO_TYPE          = psInfo.AutomationType__c;
        String I_LOADAGE            = String.valueOf(psInfo.LoadCapacity__c);                          
        //PS생성I/F
        IF_AEPortal_PS_Classes.IF_OPTY_013_Req req = new IF_AEPortal_PS_Classes.IF_OPTY_013_Req();

        List<IF_AEPortal_PS_Classes.IF_OPTY_013_Req_TB_MTS_PROP_IF> TB_MTS_PROP_IF_List = new List<IF_AEPortal_PS_Classes.IF_OPTY_013_Req_TB_MTS_PROP_IF>();

        IF_AEPortal_PS_Classes.IF_OPTY_013_Req_TB_MTS_PROP_IF TB_MTS_PROP_IF = new IF_AEPortal_PS_Classes.IF_OPTY_013_Req_TB_MTS_PROP_IF();

        TB_MTS_PROP_IF.I_INQUIRY_NO             = I_INQUIRY_NO;
        TB_MTS_PROP_IF.I_VERSION                = I_VERSION;
        TB_MTS_PROP_IF.I_RFQ_ACCEPT_DATE        = I_RFQ_ACCEPT_DATE;
        TB_MTS_PROP_IF.I_REVIEW_REQUEST_DATE    = I_REVIEW_REQUEST_DATE;
        TB_MTS_PROP_IF.I_BRANCH_NAME            = I_BRANCH_NAME;
        TB_MTS_PROP_IF.I_SALES_PERSON           = I_SALES_PERSON;
        TB_MTS_PROP_IF.I_SE_PERSON              = I_SE_PERSON;
        TB_MTS_PROP_IF.I_CUSTOMER_COMPANY       = I_CUSTOMER_COMPANY;
        TB_MTS_PROP_IF.I_DEALER_CODE            = I_DEALER_CODE;
        TB_MTS_PROP_IF.I_PRODUCT_NAME           = I_PRODUCT_NAME;
        TB_MTS_PROP_IF.I_OUTPUT                 = I_OUTPUT;
        TB_MTS_PROP_IF.I_WORKING_HOURS          = I_WORKING_HOURS;
        TB_MTS_PROP_IF.I_DUTY_DAY               = I_DUTY_DAY;
        TB_MTS_PROP_IF.I_OPERATION_RATE         = I_OPERATION_RATE;
        TB_MTS_PROP_IF.I_CYCLE_IME_REQUEST      = I_CYCLE_IME_REQUEST;
        if(I_REMARK != null){
            TB_MTS_PROP_IF.I_REMARK                 = I_REMARK.unescapeHtml4();
        }else{
            TB_MTS_PROP_IF.I_REMARK                 = I_REMARK;
        }
        TB_MTS_PROP_IF.I_EMAIL                  = I_EMAIL;
        TB_MTS_PROP_IF.I_TEL                    = I_TEL;
        TB_MTS_PROP_IF.I_WRITER                 = I_WRITER;
        TB_MTS_PROP_IF.I_IF_DATE                = I_IF_DATE;
        TB_MTS_PROP_IF.I_INQUIRY_SEQ            = I_INQUIRY_SEQ;
        // TB_MTS_PROP_IF.I_INQUIRY_SEQ            = '900';
        TB_MTS_PROP_IF.I_INQUIRY_DATE           = I_INQUIRY_DATE;
        TB_MTS_PROP_IF.I_SE_PERSON_NO           = I_SE_PERSON_NO;
        TB_MTS_PROP_IF.I_STATUS                 = i_STATUS;
        TB_MTS_PROP_IF.I_ADDRESS                = I_ADDRESS;
        TB_MTS_PROP_IF.I_INQUIRY_ID             = I_INQUIRY_ID;
        TB_MTS_PROP_IF.I_ORDER_STATUS           = I_ORDER_STATUS;
        TB_MTS_PROP_IF.I_RESULT                 = I_RESULT;
        TB_MTS_PROP_IF.I_MODEL_NAME             = I_MODEL_NAME;

        TB_MTS_PROP_IF.I_CATEGORY               = I_CATEGORY;
        TB_MTS_PROP_IF.I_MATERIAL_QUALITY       = I_MATERIAL_QUALITY;
        TB_MTS_PROP_IF.I_PRE_PROCESS            = I_PRE_PROCESS;
        TB_MTS_PROP_IF.I_BODY_TYPE              = I_BODY_TYPE;
        TB_MTS_PROP_IF.I_MACHINE_AMOUNT         = I_MACHINE_AMOUNT;
        TB_MTS_PROP_IF.I_MEASURE_TYPE           = I_MEASURE_TYPE;
        TB_MTS_PROP_IF.I_CLAMP                  = I_CLAMP;
        TB_MTS_PROP_IF.I_RECOMD_COMPANY         = I_RECOMD_COMPANY;
        TB_MTS_PROP_IF.I_AUTO_TYPE              = I_AUTO_TYPE;
        TB_MTS_PROP_IF.I_LOADAGE                = I_LOADAGE;
        TB_MTS_PROP_IF.I_RESULT_REMARK          = I_RESULT_REMARK;

        TB_MTS_PROP_IF_List.add(TB_MTS_PROP_IF);

        req.TB_MTS_PROP_IF = TB_MTS_PROP_IF_List;

        IF_AEPortal_PS instance = new IF_AEPortal_PS();
        IF_AEPortal_PS_Classes.IF_OPTY_013_Res response = instance.IF_OPTY_013(req);
        System.debug('response : ' + response);
        if(response.O_MSGTY == 'S' && IsInterfaced == false){
            PS__c updateIF = new PS__c();
            updateIF.Id = Id;
            updateIF.IsInterfaced__c = true;
            update updateIF;

            //생성 I/F할 때 첨부된 파일이 있다면 File 전송 I/F 실행
            // List<ContentDocumentLink> cdLink = [
            //     SELECT
            //         ContentDocumentId, 
            //         LinkedEntityId, 
            //         Id, 
            //         LinkedEntity.Type,
            //         ContentDocument.Title
            //     FROM
            //         ContentDocumentLink
            //     WHERE 
            //         LinkedEntityId =:Id
            // ];
            // if(cdLink.size() > 0){
            //     List<ContentDistribution> ctDis = [
            //         SELECT
            //             Name,
            //             ContentVersionId,
            //             ContentDownloadUrl
            //         FROM
            //             ContentDistribution
            //         WHERE
            //             ContentDocumentId =:cdLink[0].ContentDocumentId
            //     ];
            //     if(ctDis.size() > 0){
            //         DN_RFQFileSend.callRFQ(ctDis[0].Name,
            //                                 I_INQUIRY_SEQ,
            //                                 ctDis[0].ContentDownloadUrl,
            //                                 I_INQUIRY_ID
            //                                 );
            //     }
            // }

        }
    }
