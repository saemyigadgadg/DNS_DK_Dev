public with sharing class DN_OpportunityERPInquiryCreateBatch implements Database.Batchable<SObject>, Schedulable, Database.AllowsCallouts, Database.Stateful {
    private static final Id recordTypeIdGeneral = SObjectType.Opportunity.getRecordTypeInfosByDeveloperName().get('General').getRecordTypeId();

    public Database.QueryLocator start(Database.BatchableContext bc) {
        return Database.getQueryLocator([
            SELECT 
                Id,
                OpportunityLineItem.Opportunity.Opportunity_Owner__c, 
                OpportunityLineItem.Opportunity.AccountId, 
                OpportunityLineItem.OpportunityId, 
                OpportunityLineItem.Opportunity.Inquiry_Type__c,
                OpportunityLineItem.Opportunity.Opportunity_Owner__r.SalesOrganization__c,
                OpportunityLineItem.Opportunity.Opportunity_Owner__r.Division__c,
                OpportunityLineItem.Opportunity.Opportunity_Owner__r.Profile.UserLicense.Name, 
                OpportunityLineItem.Opportunity.Opportunity_Owner__r.Account.CustomerCode__c, 
                Product2.ProductCode, 
                OpportunityLineItem.Opportunity.RecordTypeId,
                OpportunityLineItem.Opportunity.Account.CustomerCode__c,
                OpportunityLineItemId,
                CurrencyIsoCode
            FROM 
                QuoteLineItem 
            WHERE
                // ERPInquiryNo__c = null
                OpportunityLineItem.ERPInquiryNo__c = null
            AND OpportunityLineItem.Opportunity.Opportunity_Owner__c != null
            AND Quote.RecordType.Name != 'Service'
            AND Quote.Opportunity.StageName NOT IN ('Closed Drop', 'Closed Won', 'Closed Lost')
            // AND CreatedDate = TODAY
        ]);
    }

    public void execute(Database.BatchableContext bc, List<QuoteLineItem> scope) {
        if (scope.isEmpty()) return;

        Set<Id> optyId = new Set<Id>();
        Set<Id> optyLineId = new Set<Id>();

        for(QuoteLineItem opty : scope){
            optyId.add(opty.OpportunityLineItem.OpportunityId);
        }
        for(QuoteLineItem optyLine : scope){
            optyLineId.add(optyLine.OpportunityLineItemId);
        }
        // Preload related data for optimization
        List<QuoteLineItem> quoteLineItem = [SELECT Id, ERPInquiryNo__c, OpportunityLineItemId FROM QuoteLineItem WHERE OpportunityLineItemId IN :optyLineId];
        Map<Id, QuoteLineItem> quoteLineItemMap = new Map<Id, QuoteLineItem>();
        if(!quoteLineItem.isEmpty()){
            for(QuoteLineItem qtli : quoteLineItem){
                quoteLineItemMap.put(qtli.Id, qtli);
            }
        }
        
        List<PS__c> psList = [SELECT Id, Opportunity__c, ERPInquiryNo__c FROM PS__c WHERE Opportunity__c IN :optyId];
        Map<Id, PS__c> psMap = new Map<Id, PS__c>();
        if(!psList.isEmpty()){
            for(PS__c ps : psList){
                psMap.put(ps.Opportunity__c, ps);
            }
        }

        List<OpportunityLineItem> oliUpdates = new List<OpportunityLineItem>();
        List<QuoteLineItem> qliUpdates = new List<QuoteLineItem>();
        List<PS__c> psUpdates = new List<PS__c>();

        IF_ERP_Inquiry instance = new IF_ERP_Inquiry();
        for (QuoteLineItem item : scope) {
            try {
                // Determine VTWEG based on currency
                String VTWEG = item.CurrencyIsoCode == 'KRW' ? '10' : '20';

                // Prepare ERP Inquiry request
                IF_ERP_Inquiry_Classes.IF_OPTY_006_Req req = new IF_ERP_Inquiry_Classes.IF_OPTY_006_Req();
                req.I_VKORG = item.OpportunityLineItem.Opportunity.Opportunity_Owner__r.SalesOrganization__c;
                req.I_VTWEG = VTWEG;
                req.I_SPART = item.OpportunityLineItem.Opportunity.Opportunity_Owner__r.Division__c;
                req.I_MATNR = item.Product2.ProductCode;
                req.I_KWMENG = '1';

                // Determine inquiry type
                if (recordTypeIdGeneral == item.OpportunityLineItem.Opportunity.RecordTypeId) {
                    req.I_AUART = 'ZINM';
                } else if (item.OpportunityLineItem.Opportunity.Inquiry_Type__c == 'ZIPS' || item.OpportunityLineItem.Opportunity.Inquiry_Type__c == 'ZIPA') {
                    req.I_AUART = item.OpportunityLineItem.Opportunity.Inquiry_Type__c;
                }

                // Determine customer codes
                if (item.OpportunityLineItem.Opportunity.Opportunity_Owner__r.Profile.UserLicense.Name.contains('Partner Community')) {
                    req.I_KUNNR = item.OpportunityLineItem.Opportunity.Opportunity_Owner__r.Account.CustomerCode__c;
                } else {
                    req.I_KUNNR = '9997';
                }
                req.I_KUNNR2 = item.OpportunityLineItem.Opportunity.Account.CustomerCode__c;
                System.debug('DN_OpportunityERPInquiryCreateBatch - I_KUNNR2 01 ::: ' + item.OpportunityLineItem);
                System.debug('DN_OpportunityERPInquiryCreateBatch - I_KUNNR2 02 ::: ' + req.I_KUNNR2);

                // Call ERP Inquiry service
                instance.interfaceUtil.isInsertLog = false;
                IF_ERP_Inquiry_Classes.IF_OPTY_006_Res response = instance.IF_OPTY_006(req);

                // Update OpportunityLineItem
                OpportunityLineItem upErpInquiryNo = new OpportunityLineItem(
                    Id = item.OpportunityLineItemId,
                    ERPInquiryNo__c = response.O_VBELN
                );
                oliUpdates.add(upErpInquiryNo);

                // Update related QuoteLineItem or PS__c
                if (recordTypeIdGeneral == item.OpportunityLineItem.Opportunity.RecordTypeId) {
                    QuoteLineItem quoteInfo = quoteLineItemMap.get(item.Id);
                    if (quoteInfo != null && quoteInfo.ERPInquiryNo__c == null) {
                        QuoteLineItem upInquiryNo = new QuoteLineItem(
                            Id = quoteInfo.Id,
                            ERPInquiryNo__c = response.O_VBELN
                        );
                        qliUpdates.add(upInquiryNo);
                    }
                } else if (item.OpportunityLineItem.Opportunity.Inquiry_Type__c == 'ZIPS' || item.OpportunityLineItem.Opportunity.Inquiry_Type__c == 'ZIPA') {
                    PS__c psInfo = psMap.get(item.OpportunityLineItem.OpportunityId);
                    if (psInfo != null && psInfo.ERPInquiryNo__c == null) {
                        PS__c upInquiryNo = new PS__c(
                            Id = psInfo.Id,
                            ERPInquiryNo__c = response.O_VBELN
                        );
                        psUpdates.add(upInquiryNo);
                    }
                }
            } catch (Exception e) {
                System.debug('Error processing QuoteLineItem: ' + e.getMessage());
                continue;
            }
        }
        instance.interfaceUtil.saveInterfaceLog();

        // Perform DML operations
        if (!oliUpdates.isEmpty()) update oliUpdates;
        if (!qliUpdates.isEmpty()) update qliUpdates;
        if (!psUpdates.isEmpty()) update psUpdates;
    }

    public void finish(Database.BatchableContext bc) {
        System.debug('DN_OpportunityERPInquiryCreateBatch completed :: ' + System.now());
    }

    public void execute(SchedulableContext sc) {
        Database.executeBatch(new DN_OpportunityERPInquiryCreateBatch(), 10);
    }
