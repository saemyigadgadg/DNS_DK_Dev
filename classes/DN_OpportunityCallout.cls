public with sharing class DN_OpportunityCallout {
    private static final Id   recordTypeIdGeneral  = SObjectType.Opportunity.getRecordTypeInfosByDeveloperName().get('General').getRecordTypeId();
    private static final Id   recordTypeIdProspect  = SObjectType.Account.getRecordTypeInfosByDeveloperName().get('ProspectCustomer').getRecordTypeId();
    private static final Id   recordTypeIdDealer  = SObjectType.Account.getRecordTypeInfosByDeveloperName().get('Dealer').getRecordTypeId();
    public class InquiryQueueable implements Queueable, Database.AllowsCallouts {
        private String ownerId;
        private String accountId;
        private String optyId;
        private String inquiryType;
        private List<OpportunityLineItem> prdInfo;
        private Integer currentIndex;

        public InquiryQueueable(String ownerId, String accountId, String optyId, String inquiryType, List<OpportunityLineItem> prdInfo, Integer currentIndex) {
            this.ownerId = ownerId;
            this.accountId = accountId;
            this.optyId = optyId;
            this.inquiryType = inquiryType;
            this.prdInfo = prdInfo;
            this.currentIndex = currentIndex;
        }

        public void execute(QueueableContext context) {
            if (currentIndex >= prdInfo.size()) {
                return; // 모든 Callout이 완료된 경우 종료
            }

            // 사용자 및 계정 정보 조회
            User getIFinfo = [SELECT SalesOrganization__c, Division__c, DistributionChannel__c FROM User WHERE Id =: ownerId];
            Account getIFaccinfo = [SELECT CustomerCode__c FROM Account WHERE Id =: accountId];

            Opportunity recordType = 
            [
                SELECT 
                    Id, 
                    RecordTypeId, 
                    Account.RecordTypeId,
                    AccountId,
                    Opportunity_Owner__r.CustomerCode__c,  //직영영업일 경우
                    Opportunity_Owner__r.Account.CustomerCode__c,  //딜러일경우
                    Opportunity_Owner__r.Profile.UserLicense.Name,
                    Account.SalesOrganization__c, 
                    Account.SalesDistrict__c, 
                    Account.DistributionChannel__c, 
                    Account.SalesOffice__c, 
                    Account.Division__c,
                    Opportunity_Owner__r.SalesOrganization__c, 
                    Opportunity_Owner__r.SalesDistrict__c, 
                    Opportunity_Owner__r.DistributionChannel__c, 
                    Opportunity_Owner__r.SalesOffice__c, 
                    Opportunity_Owner__r.Division__c,
                    Opportunity_Owner__r.Profile.Name,
                    Account.CustomerCode__c,
                    CurrencyIsoCode
                FROM 
                    Opportunity 
                WHERE 
                    Id =: optyId LIMIT 1
                ];
                String VTWEG;
                if(recordType.CurrencyIsoCode == 'KRW'){
                    VTWEG = '10';
                }else{
                    VTWEG = '20';
                }
                // List<SalesAreaData__c> getKUNNRFirst = 
                // [
                //     SELECT
                //         Id,
                //         SoldToParty__r.CustomerCode__c, 
                //         ShipToParty__r.CustomerCode__c,
                //         RelatedDealer__r.CustomerCode__c,
                //         Account__r.CustomerCode__c
                //     FROM
                //         SalesAreaData__c
                //     WHERE   
                //         SalesOrganization__c =: recordType.Account.SalesOrganization__c and
                //         DistributionChannel__c =: recordType.Account.DistributionChannel__c and 
                //         Division__c =: recordType.Account.Division__c and 
                //         SalesDistrict__c =: recordType.Account.SalesDistrict__c and
                //         SalesOffice__c =: recordType.Account.SalesOffice__c and
                //         Account__c =: recordType.AccountId
                //     LIMIT 1
                // ];

                // List<SalesAreaData__c> getKUNNRSec = 
                // [
                //     SELECT
                //         Id,
                //         SoldToParty__r.CustomerCode__c, 
                //         ShipToParty__r.CustomerCode__c,
                //         RelatedDealer__r.CustomerCode__c,
                //         Account__r.CustomerCode__c
                //     FROM
                //         SalesAreaData__c
                //     WHERE   
                //         SalesOrganization__c    =: recordType.Opportunity_Owner__r.SalesOrganization__c and
                //         DistributionChannel__c  =: recordType.Opportunity_Owner__r.DistributionChannel__c and 
                //         Division__c             =: recordType.Opportunity_Owner__r.Division__c and 
                //         SalesDistrict__c        =: recordType.Opportunity_Owner__r.SalesDistrict__c and
                //         SalesOffice__c          =: recordType.Opportunity_Owner__r.SalesOffice__c and
                //         Account__c =: recordType.AccountId
                //     LIMIT 1
                // ];

            // Callout 요청 생성
            OpportunityLineItem currentProduct = prdInfo[currentIndex];
            IF_ERP_Inquiry_Classes.IF_OPTY_006_Req req = new IF_ERP_Inquiry_Classes.IF_OPTY_006_Req();
            if(recordTypeIdGeneral == recordType.RecordTypeId){
                req.I_AUART = 'ZINM';
            }else if(inquiryType == 'ZIPS' || inquiryType == 'ZIPA'){
                req.I_AUART = inquiryType;
            }
            req.I_VKORG = getIFinfo.SalesOrganization__c;
            // req.I_VTWEG = getIFinfo.DistributionChannel__c;
            req.I_VTWEG = VTWEG;
            req.I_SPART = getIFinfo.Division__c;
            
            String KUNNR;
            String KUNNR2;

            // if(recordType.Opportunity_Owner__r.Profile.UserLicense.Name.contains('Partner Community')){
            if(recordType.Opportunity_Owner__r.Account.CustomerCode__c != '' && recordType.Opportunity_Owner__r.Account.CustomerCode__c != null){
                KUNNR = recordType.Opportunity_Owner__r.Account.CustomerCode__c;
            }else if(recordType.Account.RecordTypeId == recordTypeIdDealer){
                KUNNR = recordType.Account.CustomerCode__c;
            }else{
                KUNNR = '9997';
            }
            KUNNR2 = recordType.Account.CustomerCode__c;

            req.I_KUNNR = KUNNR;
            req.I_KUNNR2 = KUNNR2;
            // if (recordType.Account.RecordTypeId == recordTypeIdProspect) {
            //     if (getKUNNRFirst.size() > 0 && getKUNNRFirst[0].RelatedDealer__r.CustomerCode__c != null) {
            //         req.I_KUNNR = getKUNNRFirst[0].RelatedDealer__r.CustomerCode__c;
            //     } else if (getKUNNRSec.size() > 0 && getKUNNRSec[0].RelatedDealer__r.CustomerCode__c != null) {
            //         req.I_KUNNR = getKUNNRSec[0].RelatedDealer__r.CustomerCode__c;
            //     } else {
            //         req.I_KUNNR = '9997';
            //     }
            
            //     if (getKUNNRFirst.size() > 0 && getKUNNRFirst[0].Account__r.CustomerCode__c != null) {
            //         req.I_KUNNR2 = getKUNNRFirst[0].Account__r.CustomerCode__c;
            //     } else if (getKUNNRSec.size() > 0) {
            //         req.I_KUNNR2 = getKUNNRSec[0].Account__r.CustomerCode__c;
            //     } else {
            //         req.I_KUNNR2 = null;
            //     }
            // } else {
            //     if (getKUNNRFirst.size() > 0) {
            //         req.I_KUNNR = getKUNNRFirst[0].SoldToParty__r.CustomerCode__c;
            //         req.I_KUNNR2 = getKUNNRFirst[0].ShipToParty__r.CustomerCode__c;
            //     } else if (getKUNNRSec.size() > 0) {
            //         req.I_KUNNR = getKUNNRSec[0].SoldToParty__r.CustomerCode__c;
            //         req.I_KUNNR2 = getKUNNRSec[0].ShipToParty__r.CustomerCode__c;
            //     } else {
            //         req.I_KUNNR = null;
            //         req.I_KUNNR2 = null;
            //     }
            // }

            

            req.I_MATNR = currentProduct.ProductCode;
            req.I_KWMENG = '1';

            // Callout 실행
            IF_ERP_Inquiry instance = new IF_ERP_Inquiry();
            IF_ERP_Inquiry_Classes.IF_OPTY_006_Res response = instance.IF_OPTY_006(req);
            System.debug('Response Message: ' + response);
            System.debug('Response Message: ' + response.O_VBELN);
            OpportunityLineItem upErpInquiryNo = new OpportunityLineItem(Id = currentProduct.Id, ERPInquiryNo__c = response.O_VBELN);
            update upErpInquiryNo;
            
            
            //RecordType이 General이면
            if(recordTypeIdGeneral == recordType.RecordTypeId  || System.Test.isRunningTest()){
                List<QuoteLineItem> quoteInfo = [SELECT Id, ERPInquiryNo__c FROM QuoteLineItem WHERE OpportunityLineItemId =: currentProduct.Id];
                System.debug('quoteInfo : ' + quoteInfo);
                if(quoteInfo.size() > 0 && quoteInfo[0].ERPInquiryNo__c == null){
                    QuoteLineItem upInquiryNo = new QuoteLineItem();
                    upInquiryNo.Id = quoteInfo[0].Id;
                    upInquiryNo.ERPInquiryNo__c = response.O_VBELN;
                    update upInquiryNo;
                }
            }else if(inquiryType == 'ZIPS' || inquiryType == 'ZIPA') {
                List<PS__c> psInfo = [SELECT Id, Opportunity__c, ERPInquiryNo__c FROM PS__c WHERE Opportunity__c =: optyId];
                if(psInfo.size() > 0 && psInfo[0].ERPInquiryNo__c == null){
                    PS__c upInquiryNo = new PS__c();
                    upInquiryNo.Id = psInfo[0].Id;
                    upInquiryNo.ERPInquiryNo__c = response.O_VBELN;
                    update upInquiryNo;
                }
            }
            

            // 다음 Callout 예약
            if (currentIndex + 1 < prdInfo.size()) {
                System.enqueueJob(new InquiryQueueable(ownerId, accountId, optyId, inquiryType, prdInfo, currentIndex + 1));
            }
        }
    }

    @future(callout=true)
    public static void createInquiry(String ownerId, String accountId, String optyId, String inquiryType) {
        // 제품 정보 조회
        List<OpportunityLineItem> getIFprdinfo = [SELECT Id, ProductCode FROM OpportunityLineItem WHERE OpportunityId =: optyId and ERPInquiryNo__c = null];

        // 첫 번째 작업 예약
        System.enqueueJob(new InquiryQueueable(ownerId, accountId, optyId, inquiryType, getIFprdinfo, 0));
    }
