public with sharing class DN_ButtonHiddenController {
    @AuraEnabled
    public static Map<String, Object> hiddenInit(String recordId) {
        Map<String, Object> resMap = new Map<String, Object>();
        try {
            Schema.SObjectType objectType = Id.valueOf(recordId).getSObjectType();
            String objectName = objectType.getDescribe().getName();
            
            resMap.put('objectName', objectName);
            resMap.put('isAccountActivity', getAccountActivity(recordId));

            if(objectName == 'Quote') {
                resMap.put('getCurrentProfile', getCurrentProfile());
                resMap.put('getVersion', getVersion(recordId));
                resMap.put('isPortal', getPortalRecord(recordId).get('isPortal'));
                resMap.put('recordType', getPortalRecord(recordId).get('recordType'));
                resMap.put('worker', getPortalRecord(recordId).get('worker'));
            } else if(objectName == 'SQRegistration__c') {
                resMap.put('getSqRegistrationFilter', getSqRegistrationFilter(recordId));
            } else if(objectName == 'Lead'){
                resMap.put('getValidated', getValidated(recordId));
            }

            return resMap;
        } catch (Exception e) {
            System.debug('Error : '         + e.getMessage());
            System.debug('Line Number : '   + e.getLineNumber());
            throw new AuraHandledException(e.getMessage());
        }
    }

    private static Boolean getVersion(String recordId){
        List<Quote> quoteVersion = [SELECT Id, Recently_Version__c FROM Quote WHERE Id =: recordId];
        return quoteVersion[0].Recently_Version__c;
    }

    private static Map<String, Object> getPortalRecord(String recordId){
        List<Quote> recordType = [SELECT Id, RecordType.Name FROM Quote WHERE Id =: recordId];
        // 현재 로그인된 사용자의 프로필 정보를 가져오기
        User currentUser = [SELECT Profile.Name, Profile.UserLicense.Name, UserRole.PortalRole FROM User WHERE Id = :UserInfo.getUserId()];
        String currentUserProfile = currentUser.Profile.UserLicense.Name;

        List<Network> networkPortal = [SELECT Id, Name FROM Network WHERE Name = 'Partners'];
        Boolean isPortal = false;
        if (currentUser.Profile.UserLicense.Name.contains('Partner')) {
            isPortal = true;
        } else {
            isPortal = false;
        }
        return new Map<String, Object>{
                'isPortal' => isPortal,
                'recordType' => recordType[0].RecordType.Name,
                'worker' => currentUser.UserRole.PortalRole
            };
    }

    private static Map<String, String> getSqRegistrationFilter(String recordId) {
        Map<String, String> resMap = new Map<String, String>();
        User user = [SELECT Id, Profile.Name, Profile.UserLicense.Name FROM User WHERE Id = :UserInfo.getUserId() LIMIT 1];
        SQRegistration__c sqr = [SELECT Id, Stage__c, IsSearch__c FROM SQRegistration__c WHERE Id = :recordId LIMIT 1];
        resMap.put('stage', sqr.Stage__c);
        resMap.put('isSearch', String.valueOf(sqr.IsSearch__c));
        resMap.put('profileName', user.Profile.Name);
        resMap.put('license', user.Profile.UserLicense.Name);
        return resMap;
    }

    private static String getCurrentProfile() {
        User u = [SELECT Id, Profile.Name FROM User WHERE Id = :UserInfo.getUserId() LIMIT 1];
        return u.Profile.Name;
    }

    private static Boolean getAccountActivity(String recordId) {
        List<Account> isAccount = [SELECT Id FROM Account WHERE Id =: recordId];
        User u = [SELECT Id, isActivityAll__c FROM User WHERE Id = :UserInfo.getUserId() LIMIT 1];
        Boolean isActivity = false;
        if(!isAccount.isEmpty() && u.isActivityAll__c == false){
            isActivity = true;
        }

        List<Contact> isContact = [SELECT Id FROM Contact WHERE Id =: recordId];
        if(!isContact.isEmpty() && u.isActivityAll__c == false){
            isActivity = true;
        }

        return isActivity;
    }

    @AuraEnabled
    public static Boolean getValidated(String recordId){
        List<Lead> lead = [SELECT Id, IsERPValidated__c FROM Lead WHERE Id =: recordId];
        return lead[0].IsERPValidated__c;
    }
