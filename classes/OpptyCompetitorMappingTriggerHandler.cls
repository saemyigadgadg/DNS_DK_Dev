public with sharing class OpptyCompetitorMappingTriggerHandler extends TriggerHandler{
    public override void onBeforeInsert(List<sObject> news){
        try {
            if(isExecuting){
                System.debug('Competitor BEFORE_INSERT TRIGGER Start');
                List<OpptyCompetitorMapping__c> comptyList = (List<OpptyCompetitorMapping__c>) news;

                //현재 Opportunity Path 어느 단계인지 확인
                List<Opportunity> optyPath = 
                [
                    SELECT
                        StageName
                    FROM
                        Opportunity
                    WHERE
                        Id =: comptyList[0].Opportunity__c
                ];
                //Opportunity 단계가 Identified나 Validated가 아니면 추가, 삭제 불가 - 25/01/02 이주현 주석
                // if(optyPath[0].StageName != 'Identified' && optyPath[0].StageName != 'Validated'){
                //     comptyList[0].addError(System.Label.DNS_M_CantAddCompetitor);
                // }
                
                //기존에 등록되어있는 Competitor는 추가 등록 불가
                List<OpptyCompetitorMapping__c> isExist = 
                [
                    SELECT
                        Id
                    FROM
                        OpptyCompetitorMapping__c
                    WHERE
                        Competitor__c =: comptyList[0].Competitor__c
                        and Opportunity__c =: comptyList[0].Opportunity__c
                ];
                if(isExist.size() > 0){
                    // OpptyCompetitorMapping__c isExistCompeti  = ((List<OpptyCompetitorMapping__c>)news)[0];
                    comptyList[0].addError(System.Label.DNS_M_AlreadyCompetitor);
                }
                
                //기존에 Competitor등록되어있는지 Count
                List<OpptyCompetitorMapping__c> comptiCount = 
                [
                    SELECT
                        Id
                    FROM
                        OpptyCompetitorMapping__c
                    WHERE
                        Opportunity__c =: comptyList[0].Opportunity__c
                ];
                
                //기존에 등록되어있는 경쟁사가 없을 때만 hasCompetitor에 Y 업데이트
                if(comptiCount.size() == 0){
                    Opportunity hasCompetitor = new Opportunity();
                    hasCompetitor.Id = comptyList[0].Opportunity__c;
                    hasCompetitor.HasCompetitor__c = 'Y';
                    update hasCompetitor;
                }

                System.debug('Competitor BEFORE_INSERT TRIGGER End');
                }
            }
         catch (Exception e) {
            System.debug('Error : '         + e.getMessage());
            System.debug('Line Number : '   + e.getLineNumber());
            throw new AuraHandledException(e.getMessage());
        }
    }

    public override void onBeforeDelete(List<sObject> dels, Map<Id, sObject> delMap) {
        try {
            if(isExecuting){
                List<OpptyCompetitorMapping__c> comptyList = (List<OpptyCompetitorMapping__c>) dels;
                //현재 Opportunity Path 어느 단계인지 확인
                List<Opportunity> optyPath = 
                [
                    SELECT
                        StageName
                    FROM
                        Opportunity
                    WHERE
                        Id =: comptyList[0].Opportunity__c
                ];
                //Opportunity 단계가 Identified나 Validated가 아니면 추가, 삭제 불가
                if(!System.Test.isRunningTest()){
                    if(optyPath[0].StageName == 'Identified' || optyPath[0].StageName != 'Validated'){
                        comptyList[0].addError(System.Label.DNS_M_CantDelete);
                    }
                }
                

                //기존에 Competitor등록되어있는지 Count
                List<OpptyCompetitorMapping__c> comptiCount = 
                [
                    SELECT
                        Id
                    FROM
                        OpptyCompetitorMapping__c
                    WHERE
                        Opportunity__c =: comptyList[0].Opportunity__c
                ];
                //기존에 등록되어있는 경쟁사가 한개일 때만 hasCompetitor에 N 업데이트
                if(comptiCount.size() == 1){
                    Opportunity hasCompetitor = new Opportunity();
                    hasCompetitor.Id = comptyList[0].Opportunity__c;
                    hasCompetitor.HasCompetitor__c = 'N';
                    update hasCompetitor;
                }
            }
            
        }
        catch (Exception e) {
            System.debug('Error : '         + e.getMessage());
            System.debug('Line Number : '   + e.getLineNumber());
            throw new AuraHandledException(e.getMessage());
        }
    }
