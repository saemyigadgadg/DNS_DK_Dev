public with sharing class DN_QuoteReviewController {
    @AuraEnabled
    public static string roleCheck(String recordId){
        try {
            List<Quote> recordType = [SELECT Id, RecordType.Name FROM Quote WHERE Id =: recordId];
            // 현재 로그인된 사용자의 프로필 정보를 가져오기
            User currentUser = [SELECT Profile.Name, Profile.UserLicense.Name, UserRole.PortalRole FROM User WHERE Id = :UserInfo.getUserId()];
            String currentUserProfile = currentUser.Profile.UserLicense.Name;
    
            List<Network> networkPortal = [SELECT Id, Name FROM Network WHERE Name = 'Partners'];
            Boolean isPortal = false;
            if (currentUser.Profile.UserLicense.Name.contains('Partner')) {
                isPortal = true;
            } else {
                isPortal = false;
            }
            //딜러인데 Worker이고, Global이면 Review 이용 못함
            if(isPortal && recordType[0].RecordType.Name == 'Global' && currentUser.UserRole.PortalRole == 'Worker'){
                return System.Label.DNS_M_DontUseReview;
            }else{
                return 'SUCCESS';
            }
        } catch (Exception e) {
            throw new AuraHandledException(e.getMessage());
        }
    }

    @AuraEnabled
    public static string reviewRequest(String recordId, String requestReason){
        try {
            Quote upRecord = new Quote();
            upRecord.Id = recordId;
            upRecord.Review__c = requestReason;
            update upRecord;

            User currentUser = [SELECT Profile.Name, Profile.UserLicense.Name, UserRole.PortalRole FROM User WHERE Id = :UserInfo.getUserId()];
            String currentUserProfile = currentUser.Profile.UserLicense.Name;

            //글로벌본사 직원에게 Noti
            List<Quote> quoteInfo = [
                    SELECT 
                        Id, 
                        Review__c, 
                        Name,
                        QuoteNumber,
                        Opportunity.Opportunity_Owner__c, 
                        Opportunity.Opportunity_Owner__r.Manager.Email, 
                        Opportunity.Opportunity_Owner__r.ManagerId, 
                        Opportunity.Opportunity_Owner__r.LanguageLocaleKey,
                        Opportunity.Opportunity_Owner__r.Manager.LanguageLocaleKey
                    FROM 
                        Quote 
                    WHERE 
                        Id =: recordId
                        ];
            Set<Id> ownerId = new Set<Id>();
            ownerId.add(quoteInfo[0].Opportunity.Opportunity_Owner__c);
            List<User> getEmail =  DN_WithoutSharingClass.getUserEmails(ownerId);
            
            // List<Network> portal = [SELECT Id, Name FROM Network WHERE Name = 'Partners'];
            // String instanceUrl = Network.getLoginUrl(portal[0].Id).removeEnd('/s/login');
            //Review담당자는 CRM User
            String instanceUrl = URL.getOrgDomainUrl().toExternalForm();
            List<AlertManager__c> am = [SELECT Id FROM AlertManager__c WHERE Name = 'QuoteRequestReview'];
            Id alertId;

            if(!am.isEmpty()){
                alertId = am[0].Id;
            }else{
                alertId = 'a0xF7000009v9HgIAI';
            }
            UTIL_Alert.HistoryWrapper wrapEmail = new UTIL_Alert.HistoryWrapper();
                wrapEmail.alertID            = alertId;
                wrapEmail.targetID           = recordId;
                wrapEmail.alertType          = 'Email';
                wrapEmail.receiverforEmail   = new List<String>{getEmail[0].Manager.Email};
                // wrapEmail.receiverforEmail   = new List<String>{'sejin.kwon@dncompany.com'};
                wrapEmail.alertLanguage      = 'ko';
                wrapEmail.elementList        = new List<String>{instanceUrl+'/'+recordId};

            UTIL_Alert.HistoryWrapper wrapNoti = new UTIL_Alert.HistoryWrapper();
                wrapNoti.alertID            = alertId;
                wrapNoti.targetID           = recordId;
                wrapNoti.alertType          = 'Notification';
                wrapNoti.receiverforID      = new Set<Id>{quoteInfo[0].Opportunity.Opportunity_Owner__r.ManagerId};
                // wrapNoti.receiverforID      = new Set<Id>{'005F7000006DOYtIAO'};
                wrapNoti.alertLanguage      = 'ko';
                wrapNoti.elementList        = new List<String>{quoteInfo[0].QuoteNumber};

            if(quoteInfo[0].Opportunity.Opportunity_Owner__r.Manager.LanguageLocaleKey == 'en_US'){
                wrapEmail.alertLanguage      = 'en';
                wrapNoti.alertLanguage      = 'en';
            }
            UTIL_Alert.runAlertEvent(new List<UTIL_Alert.HistoryWrapper>{wrapEmail,wrapNoti});
            return 'SUCCESS';
        } catch (Exception e) {
            throw new AuraHandledException('Error: ' + e.getMessage());
        }
    }
