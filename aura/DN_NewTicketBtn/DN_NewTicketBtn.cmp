                                        <!--ê¸°ìˆ ìƒë‹´-->
                                        <aura:if
                                            isTrue="{!(t.TicketType__c) &amp;&amp; !(t.TicketType__c == '' || t.TicketType__c == 'General inquiry')}">
                                            <h2 class="infoTitle">{!$Label.c.TechnicalConsultation}</h2>
                                            <div class="field-wrap">
                                                <!--ê³ ìž¥ë¶€ìœ„-->
                                                <lightning:combobox label="{!$Label.c.DNS_F_FailureArea}"
