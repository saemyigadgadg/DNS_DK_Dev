                                                <lightning:inputField fieldName="ReceptionPath__c"
                                                    value="{!t.ReceptionPath__c}" />
                                                <!--ì ‘ìˆ˜ì¼ì‹œ-->
                                                <lightning:inputField fieldName="ApplicationDateTime__c"
