<aura:component implements="forceCommunity:availableForAllPageTypes,force:lightningQuickActionWithoutHeader,force:hasRecordId,force:hasSObjectName,lightning:actionOverride"
    controller="DN_CustomerManagementModalController" access="global"
>
<aura:handler name="init" value="{! this }" action="{! c.doInit }" />
<aura:attribute name="recordId" type="String" />
<aura:attribute name="sObjectName" type="String" />
<aura:attribute name="isCheck" type="Boolean" default="true" />
    <aura:if isTrue="{! v.isCheck}"> 
        <aura:set attribute="else">
            <c:DN_AccountAddressPopUp recordId="{!v.recordId}" sObjectName ="{!v.sObjectName}"/>
        </aura:set>
    </aura:if>

</aura:component>